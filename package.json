{
  "name": "topaz-signature-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "copy-dlls": "mkdirp dist-electron\\libs && copyfiles -f libs/*.dll dist-electron/libs/ && dir dist-electron\\libs",
    "electron": "tsc -p tsconfig.electron.json && npm run copy-dlls && electron --trace-warnings ./dist-electron/electron/main.js",
    "dev:electron": "concurrently \"vite\" \"npm run electron\""
  },
  "dependencies": {
    "@felipealexandre/signature-lib": "^1.3.3",
    "@tailwindcss/vite": "^4.1.7",
    "axios": "^1.9.0",
    "electron-edge-js": "^36.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-number-format": "^5.4.4",
    "react-router-dom": "^7.6.0",
    "tailwindcss": "^4.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "electron": "^36.3.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "mkdirp": "^3.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}